{
    "name": "ensemble-transposer",
    "version": "3.0.0",
    "private": true,
    "description": "ensemble-transposer re-formats existing data so that it can be used by the Firefox Public Data Report.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mozilla/ensemble-transposer.git"
    },
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/mozilla/ensemble-transposer/issues"
    },
    "homepage": "https://github.com/mozilla/ensemble-transposer#readme",
    "engines": {
        "node": ">=8.0.0"
    },
    "main": "transpose/index.js",
    "scripts": {
        "start": "scripts/nvmm 'node start'",
        "dev": "NODE_ENV=development scripts/nvmm 'node start'",
        "test": "npm-run-all lint mocha:setup mocha:standard",
        "lint": "eslint --ext=.js,.json .",
        "mocha:setup": "NODE_ENV=test scripts/nvmm 'node start'",
        "mocha:standard": "NODE_ENV=test scripts/nvmm 'mocha tests/standard'",
        "compare": "npm-run-all mocha:setup mocha:api-equivalence",
        "mocha:api-equivalence": "NODE_ENV=test scripts/nvmm 'mocha tests/special/api-equivalence.test.js'",
        "posttest": "npm audit || true"
    },
    "dependencies": {
        "aws-sdk": "2.545.0",
        "big.js": "5.2.2",
        "dotenv": "8.1.0",
        "memoizee": "0.4.14",
        "request": "2.88.0",
        "request-promise-native": "1.0.7"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "deep-equal-in-any-order": "1.0.21",
        "eslint": "6.5.1",
        "eslint-plugin-json": "1.4.0",
        "eslint-plugin-mocha": "6.1.1",
        "eslint-plugin-node": "10.0.0",
        "mkdirp": "0.5.1",
        "mocha": "6.2.1",
        "npm-run-all": "4.1.5"
    }
}
