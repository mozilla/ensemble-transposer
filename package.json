{
    "name": "ensemble-transposer",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "node server",
        "dev": "docker-compose --file docker-compose.dev.yml up",
        "stopdev": "docker-compose --file docker-compose.dev.yml stop",
        "redis-cli": "docker-compose --file docker-compose.dev.yml run redis-cli",
        "build": "node scripts/generateVersionJSON",
        "validate": "npm run test",
        "test": "npm run lint",
        "lint": "eslint --ext=.js,.json .",
        "posttest": "npm audit || true"
    },
    "dependencies": {
        "cors": "2.8.4",
        "express": "4.16.4",
        "express-force-ssl": "0.3.2",
        "helmet": "3.13.0",
        "morgan": "1.9.1",
        "redis": "2.8.0",
        "request": "2.88.0"
    },
    "devDependencies": {
        "eslint": "5.6.1",
        "eslint-plugin-json": "1.2.1",
        "eslint-plugin-node": "7.0.1",
        "nodemon": "1.18.4"
    }
}
